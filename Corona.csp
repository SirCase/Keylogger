	.EQU PASSWORD, $030
	.EQU	@, PASSWORD
	.CHAR 'get pwned', LENGTH
	.EQU KEYBOARD_STATUS, $000
	.EQU KEYBOARD_DATA, $001

	.EQU @, $000
	
	LDF#	$000
POLL:	LDX#	$000
	INB	KEYBOARD_STATUS
	NOP
	JGE	POLL
	INB 	KEYBOARD_DATA
	STA ! $D00
	ADF#	$001
	LDX#	$000
CHECK:	LDC	PASSWORD
	CMA+ !$D00-LENGTH
	JNE	POLL
	AOC#	LENGTH
	JGE	DSETUP
	JMP 	CHECK

DSETUP:	LDX#	$000
DESTROY:	CLR+	$000
	ADX#	$001
	JMP 	DESTROY