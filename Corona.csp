	.EQU PASSWORD, $030
	.EQU	@, PASSWORD
	.CHAR 'get pwned', LENGTH
	.EQU KEYBOARD_STATUS, $000
	.EQU KEYBOARD_DATA, $001

	.EQU @, $000
	LDX#	$000
	LDF#	$000
START:	LDA+ !$CFE
	CMA+	PASSWORD
	JNE	POLL
	AOC	LENGTH
	JGE	DESTROY
	JMP 	START
	
POLL:	INB	KEYBOARD_STATUS
	NOP
	JGE	POLL
	INB 	KEYBOARD_DATA
	STA	!$D00
	ADF#	$001

DESTROY:	CLR+	$000
	ADX#	$001
	JMP 	DESTROY